@namespace("eu.dnetlib.iis.workflows.top.schemas")
protocol IIS{

	record Agent {
		union { null , string } id = null;
		union { null , string } name = null;
		union { null , string } type = null;
	}

	record ActionObject {

		union { null , bytes } rowKey = null;
		union { null , bytes } collumnFamily = null;
		union { null , bytes } collumnQualifier = null;
		union { null , bytes } value = null;
		union { null , string } actionSet = null;
		union { null , Agent } agent = null;

	}

}